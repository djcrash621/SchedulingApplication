<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="345.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.add_customer_controller">
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="170.0" layoutY="14.0" text="Add Customer" textAlignment="CENTER" />
   <Label layoutX="69.0" layoutY="59.0" text="Name:" />
   <TextField fx:id="customerNameField" layoutX="124.0" layoutY="55.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="61.0" layoutY="91.0" text="Address:" />
   <TextField fx:id="addressField" layoutX="124.0" layoutY="87.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="43.0" layoutY="123.0" text="Postal Code:" />
   <TextField fx:id="postalCodeField" layoutX="124.0" layoutY="119.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="27.0" layoutY="156.0" text="Phone Number:" />
   <TextField fx:id="phoneNumField" layoutX="124.0" layoutY="152.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="64.0" layoutY="234.0" text="Division:" />
   <Button fx:id="saveNewCustomer" layoutX="314.0" layoutY="287.0" mnemonicParsing="false" onAction="#saveCustomer" text="Save" />
   <Button fx:id="cancelBtn" layoutX="250.0" layoutY="287.0" mnemonicParsing="false" onAction="#returnToWelcome" text="Cancel" />
   <ComboBox fx:id="divisionComboBox" layoutX="124.0" layoutY="230.0" prefHeight="25.0" prefWidth="98.0" />
   <Label layoutX="64.0" layoutY="192.0" text="Country:" />
   <ComboBox fx:id="countryComboBox" layoutX="124.0" layoutY="188.0" onAction="#filterByCountry" prefHeight="25.0" prefWidth="98.0" />
</AnchorPane>
