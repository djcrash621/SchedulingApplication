<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.edit_customer_controller">
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="170.0" layoutY="14.0" text="Edit Customer" textAlignment="CENTER" />
   <Label layoutX="28.0" layoutY="59.0" text="Name" />
   <TextField fx:id="customerNameField" layoutX="124.0" layoutY="55.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="28.0" layoutY="91.0" text="Address" />
   <TextField fx:id="addressField" layoutX="124.0" layoutY="87.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="28.0" layoutY="123.0" text="Postal Code" />
   <TextField fx:id="postalCodeField" layoutX="124.0" layoutY="119.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="28.0" layoutY="156.0" text="Phone Number" />
   <TextField fx:id="phoneNumField" layoutX="124.0" layoutY="152.0" prefHeight="25.0" prefWidth="232.0" />
   <Label layoutX="28.0" layoutY="191.0" text="Division" />
   <ComboBox fx:id="divisionComboBox" layoutX="125.0" layoutY="188.0" prefHeight="25.0" prefWidth="232.0" />
   <Button fx:id="saveNewCustomer" layoutX="304.0" layoutY="361.0" mnemonicParsing="false" onAction="#saveCustomer" text="Save" />
   <Button fx:id="cancelBtn" layoutX="240.0" layoutY="361.0" mnemonicParsing="false" onAction="#returnToWelcome" text="Cancel" />
</AnchorPane>
